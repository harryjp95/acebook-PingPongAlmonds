<h1>New users page</h1>

<%= form_with scope: :user, url: users_path, local: true do |form| %>
  <p>
    <%= form.label :Email %>
      <br>
      <%= form.text_field :email %>
  </p>

  <p>
    <%= form.label :Password %>
      <br>
      <%= form.text_field :password %>
  </p>

  <p>
    <%= form.submit %>
  </p>
  <% end %>
